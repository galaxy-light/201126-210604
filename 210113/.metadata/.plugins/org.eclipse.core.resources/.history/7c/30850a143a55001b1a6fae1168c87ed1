
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car car = new Car(); // 객체 생성 / ()가 있으면 메서드
		
		// 객체 안에 들어와 있는 인스턴스 정보 출력
		car.showCarInfo();
		
		Car car2 = new Car("그랜저", "현대", "흰색", "4천만원"); // 객체를 생성할 때만 사용되어지는 메서드
		car2.showCarInfo();
	}

}
