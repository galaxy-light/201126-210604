package com.y.carApp.car;

public class Car {
	private String model;
	private int year;
	private String color;
	private String company;
	
	// 매개변수를 사용하는 생성자
	public Car(String model, int year, String color, String company) {
		super();
		this.model = model;
		this.year = year;
		this.color = color;
		this.company = company;
	}
	
	
	
	
}
