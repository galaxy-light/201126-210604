package com.y.stApp;

public class Student {
	// 속성 선언, 다른 클래스의 접근 차단
	private String name;
	private int age;
	private char gender;
	private String address;
	
	// 매개변수를 사용하는 생성자
	public Student(String name, int age, char gender, String address) {
		super(); // 부모 클래스 : Object
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.address = address;
	}
	
}
