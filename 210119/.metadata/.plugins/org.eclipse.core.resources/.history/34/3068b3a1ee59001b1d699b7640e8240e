package com.y.stApp;

public class Student {
	private String name;
	private int age;
	private char gender;
	private String address;
	
	// 매개변수를 사용하는 생성자
	public Student(String name, int age, char gender, String address) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.address = address;
	}
	
}
