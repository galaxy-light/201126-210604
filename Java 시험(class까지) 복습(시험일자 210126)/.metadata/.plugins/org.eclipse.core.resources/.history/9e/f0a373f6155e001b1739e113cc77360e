	}

	public static void func(int arr) {
		Random rand = new Random();
		int arr[] = new int [6];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = rand.nextInt(45)+1;
//			System.out.println(arr[i]);
		}
	}
}
